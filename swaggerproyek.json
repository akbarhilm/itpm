                },
                "namaapl": {
                    "type": "string",
                    "example": "Aplikasi xxxx",
                    "description": "Nama Aplikasi"
                },
                "ketapl": {
                    "type": "string",
                    "example": "Aplikasi untuk xxxxxxx",
                    "description": "Keterangan dari aplikasi"
                },
                "aktif": {
                    "type": "string",
                    "example": "1",
                    "description": "Kode Aktif"
                }
            },
