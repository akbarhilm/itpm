                },
                "namamodul": {
                    "type": "string",
                    "example": "Modul xxxx",
                    "description": "Nama Modul dari aplikasi"
                },
                "ketmodul": {
                    "type": "string",
                    "example": "Modul untuk xxxxxxx",
                    "description": "Keterangan Modul dari aplikasi"
                },
                "aktif": {
                    "type": "string",
                    "example": "1",
                    "description": "Kode Aktif"
                }
            },
            "title": "Request Modul"
