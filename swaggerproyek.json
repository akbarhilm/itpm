    "definitions": {
        "Request Proyek": {
            "type": "object",
            "required": [
                "idlayanan",
                "statusapl",
                "jenislayanan",
                "namaproj",
                "namauri",
                "nikreq",
                "nikpm",
                "ketproj"
            
            ],
            "properties": {
                "idlayanan": {
                    "type": "integer",
                    "example": "1",
                    "description": "id dari Layanan"
                },
                "statusapl": {
                    "type": "string",
                    "enum":[
                        "SAP",
                        "NON_SAP"
                    ],
                    "example": "SAP"
                },
                "jenislayanan": {
                    "type": "string",
                    "example": "PENGEMBANGAN",
                    "enum":[
                        "PENGEMBANGAN",
                        "PERUBAHAN"
                    ]
                    
                },
                "namaproj": {
                    "type": "string",
                    "example": "Projek xxxxx",
                    "description": "Nama Project"
                },
                "ketproj": {
                    "type": "string",
                    "example": "projek untuk bla bla bla bah",
                    "description": "Deskripsi projek"
                },
                
                "namauri": {
                    "type": "string",
                    "example": "itpm",
                    "description": "URI Project"
                },
                "nikreq": {
                    "type": "string",
                    "example": "18xxxx",
                    "description": "Karyawan Peminta Layanan"
                },
                "nikpm": {
                    "type": "string",
                    "example": "18xxxx",
                    "description": "NIK Karyawan Project Manager"
                },
                "idaplikasi": {
                    "type": "integer",
                    "required": false,
                    "example": "1 (remove this key if not used)",
                    "description": "id aplikasi"
                },
                "idmodul": {
                    "type": "integer",
                    "required": false,
                    "example": "1 (remove this key if not used)",
                    "description": "id modul dari aplikasi"
                }
            },
            "title": "Request Layanan"
        },
