                    "type": "string"
                },
                "N_ITPM_PROJURI": {
                    "type": "string"
                },
                "I_EMP_REQ": {
                    "type": "string"
                },
                "I_EMP_PM": {
                    "type": "string"
                },
                "I_ITPM_RISKNBR": {
                    "type": "string"
                },
                "I_ITPM_PLANNBR": {
                    "type": "string"
                },
                "I_ITPM_RESRCNBR": {
                    "type": "string"
                },
                "I_ITPM_REALNBR": {
                    "type": "string"
                },
                "I_ITPM_BANBR": {
                    "type": "string"
                },
                "D_ITPM_BA": {
                    "type": "string"
                },
                "C_ITPM_BAAPPRV": {
                    "type": "string"
                },
                "D_ITPM_BAAPPRV": {
                    "type": "string"
                },
                "I_ITPM_UREQNBR": {
